# Copy this file to .env.local and fill in your actual Supabase credentials
# You can get these from your Supabase project dashboard

# Supabase Configuration (Required for voice chat)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# TURN Server Configuration (Optional - for better NAT traversal)
NEXT_PUBLIC_METERED_TURN_URL=turn:relay.metered.ca:80
NEXT_PUBLIC_METERED_TURN_USERNAME=your-metered-username
NEXT_PUBLIC_METERED_TURN_CREDENTIAL=your-metered-credential
